{
  "name": "react-sandbox",
  "author": "jmsmrgn",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "history": "^4.4.1",
    "lodash": "^4.17.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^4.0.0-alpha.6"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-plugin-babel": "^4.0.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.2",
    "opt-cli": "^1.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.27",
    "webpack-config-utils": "^2.1.0",
    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-validator": "^2.2.11"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "webpack -p --env.prod",
    "prebuild:dev": "rimraf build",
    "build:dev": "webpack --env.dev",
    "dev": "webpack-dev-server --open --env.dev",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "validate": "npm-run-all --parallel lint build",
    "lint": "eslint .",
    "start": "http-server ./_build"
  }
}
